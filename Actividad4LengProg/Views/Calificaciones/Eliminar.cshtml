@model Actividad4LengProg.Models.Calificacione

<h2>Eliminar Calificación</h2>

<div>
    <h4>¿Está seguro que desea eliminar esta calificación?</h4>
    <dl class="row">
        <dt class="col-sm-2">Estudiante</dt>
        <dd class="col-sm-10">@Model.MatriculaEstudianteNavigation.NombreCompleto</dd>

        <dt class="col-sm-2">Materia</dt>
        <dd class="col-sm-10">@Model.CodigoMateriaNavigation.Nombre</dd>

        <dt class="col-sm-2">Nota</dt>
        <dd class="col-sm-10">@Model.Nota</dd>

        <dt class="col-sm-2">Periodo</dt>
        <dd class="col-sm-10">@Model.Periodo</dd>
    </dl>

    <form asp-action="Eliminar" method="post">
        @Html.AntiForgeryToken()
        <input type="hidden" asp-for="Id" />
        <button type="submit" class="btn btn-danger">Eliminar</button>
        <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
    </form>
</div>
